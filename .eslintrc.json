{
    "extends": "eslint:recommended",
    "installedESLint": true,
    "plugins" : ["react"], 
    "rules" : {
        "linebreak-style": ["warn", "unix"],
        "no-undef": "warn",
        "no-unused-vars": "warn"
    },
    "parserOptions": {
        "ecmaVersion":6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}